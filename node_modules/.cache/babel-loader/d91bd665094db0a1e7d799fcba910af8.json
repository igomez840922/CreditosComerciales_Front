parklinesCurve\r\n                    style={{\r\n                      strokeWidth: 0.8,\r\n                      lineColor: '#5b73e8',\r\n                      fillColor: 'transparent',\r\n                      spotColor: '#5b73e8',\r\n                    }}\r\n                  />\r\n                  <SparklinesBars style={{ fill: \"#3c4ccf\" }} barWidth={8} />\r\n                </Sparklines>\r\n              </CardBody>\r\n            </Card>\r\n          </Col>\r\n          <Col lg=\"4\" sm=\"6\">\r\n            <Card>\r\n              <CardBody>\r\n                <CardTitle className=\"mb-4\">Reference Line Charts</CardTitle>\r\n                <Sparklines\r\n                  height={100}\r\n                  data={[\r\n                    5,\r\n                    10,\r\n                    5,\r\n                    20,\r\n                    18,\r\n                    17,\r\n                    29,\r\n                    10,\r\n                    18,\r\n                    15,\r\n                    20,\r\n                    18,\r\n                    17,\r\n                    29,\r\n                    10,\r\n                    18,\r\n                  ]}\r\n                >\r\n                  <SparklinesBars\r\n                    style={{ fill: \"#02a499\", fillOpacity: \".5\" }}\r\n                  />\r\n                  <SparklinesReferenceLine />\r\n                </Sparklines>\r\n              </CardBody>\r\n            </Card>\r\n          </Col>\r\n          <Col lg=\"4\" sm=\"6\">\r\n            <Card className=\"bg-success mb-4\">\r\n              <CardBody>\r\n                <CardTitle className=\"h4 mb-4\">Real World Chart</CardTitle>\r\n                <Sparklines\r\n                  height={80}\r\n                  data={[\r\n                    4,\r\n                    6,\r\n                    7,\r\n                    7,\r\n                    4,\r\n                    3,\r\n                    2,\r\n                    1,\